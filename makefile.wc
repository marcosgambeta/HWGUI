OBJ_DIR = obj
LIB_DIR = lib
SRC_DIR = source
CONTRIB_DIR = contrib
#C_DEFINES= -DHB_THREAD_SUPPORT
#H_DEFINES= -DHB_THREAD_SUPPORT

